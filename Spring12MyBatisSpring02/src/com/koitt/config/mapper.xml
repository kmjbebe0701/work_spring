  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
  <mapper namespace="com.koitt.dao.PetMapper">
  	<resultMap type="Pet" id="PetResultMap">
  		<id property="petId" column="pet_id"/>
